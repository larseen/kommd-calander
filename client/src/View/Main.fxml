<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<StackPane minHeight="400.0" minWidth="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainController">
   <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints percentWidth="100" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="100" vgrow="NEVER" />
          <RowConstraints vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <StackPane>
               <children>
                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="144.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="10.0" prefWidth="110.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="126.0" minWidth="10.0" prefWidth="88.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.0" minWidth="10.0" prefWidth="178.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Button fx:id="home" mnemonicParsing="false" onAction="#onHome" prefHeight="111.0" prefWidth="145.0" style="-fx-background-color: #FFFFFF;" text="HOME">
                           <font>
                              <Font name="System Bold" size="32.0" />
                           </font>
                        </Button>
                        <Button fx:id="calendar" mnemonicParsing="false" onAction="#onCalendar" prefHeight="105.0" prefWidth="136.0" style="-fx-background-color: #FFFFFF;" text="Calendar" GridPane.columnIndex="1">
                           <font>
                              <Font size="22.0" />
                           </font>
                        </Button>
                        <Button fx:id="groups" mnemonicParsing="false" onAction="#onGroups" prefHeight="111.0" prefWidth="138.0" style="-fx-background-color: #FFFFFF;" text="Groups" GridPane.columnIndex="2">
                           <font>
                              <Font size="22.0" />
                           </font>
                        </Button>
                        <Button fx:id="rooms" mnemonicParsing="false" onAction="#onRooms" prefHeight="111.0" prefWidth="139.0" style="-fx-background-color: #FFFFFF;" text="Rooms" GridPane.columnIndex="3">
                           <font>
                              <Font size="22.0" />
                           </font>
                        </Button>
                        <GridPane GridPane.columnIndex="5">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="82.0" minWidth="10.0" prefWidth="68.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="10.0" prefWidth="110.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label fx:id="currentUserLabel" prefHeight="21.0" prefWidth="81.0" text="currentUser" GridPane.columnIndex="1" />
                              <Button fx:id="userMenu" mnemonicParsing="false" onAction="#onUserMenu" prefHeight="55.0" prefWidth="33.0" style="-fx-background-color: #FFFFFF;">
                                 <graphic>
                                    <ImageView fitHeight="41.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" style="-fx-image: url('http://icons.iconarchive.com/icons/aha-soft/desktop-buffet/128/Pizza-icon.png');" />
                                 </graphic>
                              </Button>
                           </children>
                        </GridPane>
                     </children>
                  </GridPane>
               </children>
            </StackPane>
            <StackPane fx:id="displayView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</StackPane>
